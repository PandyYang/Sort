package pandy.test.tree;

public class TestTree {
	public static void main(String[] args) {
		Tree tree = new Tree();
		tree.insert(5,"pandy5");
		tree.insert(7,"pandy7");
		tree.insert(6,"pandy6");
		tree.insert(9,"pandy9");
		tree.insert(8,"pandy8");
		tree.insert(1,"pandy1");
		tree.insert(3,"pandy3");
		
		System.out.println(tree.root.data);
		System.out.println(tree.root.leftChild.data);
		System.out.println(tree.root.rightChild.leftChild.data);
		//因为节点中包含有数据 所以查找到的数据就是节点
		Node node = tree.find(1);
		System.out.println(node.data+"------------>"+node.sData);
	}
}
